{
    "name": "jawor-parkiet/shopping-cart",
    "description": "Laravel Shopping Cart dedicated to Jawor-Parkiet E-commerce",
    "keywords": [
        "laravel",
        "shopping cart"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Matt Komarnicki",
            "email": "matt@mattkomarnicki.com"
        }
    ],
    "require": {
        "php": "^8.3.20",
        "illuminate/support": "12.*",
        "illuminate/session": "12.*",
        "illuminate/events": "12.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~11.0",
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "JaworParkiet\\ShoppingCart\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JaworParkiet\\Tests\\ShoppingCart\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "JaworParkiet\\ShoppingCart\\ShoppingCartServiceProvider"
            ],
            "aliases": {
                "Cart": "JaworParkiet\\ShoppingCart\\Facades\\Cart"
            }
        }
    }
}
